# Example environment configuration
# Copy this file to .env.local for development
# For production deployment, set these values in your GitHub Pages deployment environment

# API Configuration
VITE_API_BASE_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
VITE_API_KEY=your-optional-api-key
VITE_ALLOWED_ORIGINS=https://yourusername.github.io
VITE_ENVIRONMENT=development

# Security Configuration (optional)
VITE_ENABLE_API_ENCRYPTION=false
VITE_API_TIMEOUT=10000
